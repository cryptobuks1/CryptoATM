<Window x:Class="ATM.DispenseCash"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ATM"
        mc:Ignorable="d"
        Title="DispenseCash" Height="450" Width="800" WindowState="Maximized">
    <Grid Background="#FF244C91">
        <StackPanel Margin="10,20,10,0" VerticalAlignment="Center">
            <TextBlock x:Name="lblmsg" FontFamily="Segoe Print" FontSize="20" Padding="3" Foreground="#FFE1F704" HorizontalAlignment="Center"/>
        </StackPanel>
    </Grid>
</Window>
